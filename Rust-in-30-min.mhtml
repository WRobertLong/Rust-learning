<i class=3D"hh4">use</i> std<i class=3D"hh9">::</i>cmp<i class=3D"hh9">::</=
i>min<i class=3D"hh9">;</i>
<i class=3D"hh4">use</i> std<i class=3D"hh9">::</i>cmp<i class=3D"hh9">::</=
i>max<i class=3D"hh9">;</i>

<i class=3D"hh18">// this also works:</i>
<i class=3D"hh4">use</i> std<i class=3D"hh9">::</i>cmp<i class=3D"hh9">::</=
i><i class=3D"hh8">{</i>min<i class=3D"hh9">,</i> max<i class=3D"hh8">}</i>=
<i class=3D"hh9">;</i>

